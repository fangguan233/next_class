# 配置文件
#分享码保存时长（单位小时）
SHARE_CONFIG_EXPIRATION_HOURS=24
#分享码访问频率限制（单位秒）
SHARE_RATE_LIMIT_SECONDS=60

# 功能开关
# 图片识别功能尚处于实验阶段，识别效果非常不稳定。设置为 true 以启用。
ENABLE_IMAGE_PROCESSING=false

# 开发者模式。设置为 true 时，前端会在 ETag 不匹配时自动清除缓存并刷新。
# 设置为 false 时，则只更新 ETag，不主动刷新，适用于生产环境。
DEV_MODE=false

# --- 服务器运行模式配置 ---
# 设置为 true 以强制启用 HTTPS 模式，false 则使用 HTTP 模式。
ENABLE_HTTPS=false

# HTTP 模式下使用的端口
HTTP_PORT=80

# HTTPS 模式下使用的端口
HTTPS_PORT=443

# --- 管理后台配置 ---
# 管理后台运行的端口
ADMIN_PORT=5001

# --- ICP备案信息 (可选) ---
# 如果您已完成网站备案，请在此处填写您的备案号。
# 备案号将显示在所有页面的页脚，并链接到工信部网站。
# 如果留空，则不显示。
ICP_LICENSE=

